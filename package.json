{
  "name": "@project-eleven/dep-doc",
  "version": "0.0.0",
  "description": "A Typescript library for linting dependency documentation.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Conor Deegan",
  "type": "commonjs",
  "bin": {
    "dep-doc": "dist/index.js"
  },
  "keywords": [
    "dependency",
    "dep-doc",
    "dependency management"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "nodemon --watch src --exec ts-node src/index.ts",
    "clean": "rimraf dist",
    "build": "tsc -b",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:check": "eslint --config eslint.config.mjs \"./src/*.ts\"",
    "lint:fix": "eslint --config eslint.config.mjs \"./src/*.ts\" --fix",
    "prettier:check": "prettier --config .prettierrc \"./**/*.+(js|json|ts|mjs)\" --check",
    "prettier:fix": "prettier --config .prettierrc \"./**/*.+(js|json|ts|mjs)\" --write",
    "check": "npm run lint:check && npm run prettier:check && npm test",
    "release:patch": "npm version patch -m \"Release v%s\"",
    "release:minor": "npm version minor -m \"Release v%s\"",
    "release:major": "npm version major -m \"Release v%s\""
  },
  "homepage": "https://github.com/p-11/dep-doc#readme",
  "bugs": {
    "url": "https://github.com/p-11/dep-doc/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/p-11/dep-doc.git"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "toml": "^3.0.0",
    "zod": "^4.1.12"
  }
}
